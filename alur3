ffunction setup() {
  createCanvas(600, 600);
  background("black");
}

function draw() {
  stroke("gold");
  fill("gold");

  //console.log(mouseIsPressed)

  if (mouseIsPressed) rect(mouseX, mouseY, 135, 35);

  fill("gold");
}unction setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
}
